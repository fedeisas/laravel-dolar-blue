    "require-dev" : {
        "phpunit/phpunit": "3.7.*",
        "mockery/mockery": "0.9.*",
        "squizlabs/php_codesniffer": "1.*",
        "jakub-onderka/php-parallel-lint": "dev-master",
        "jakub-onderka/php-console-highlighter": "dev-master",
        "pdepend/pdepend" : "1.1.1",
        "phpmd/phpmd": "1.5.*",
        "php-vcr/php-vcr": "dev-master",
        "php-vcr/phpunit-testlistener-vcr": "dev-master"
    },
    "autoload": {
        "psr-4": {
